rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /events/{eventId}/{allPaths=**} {
      allow read: if true;
      allow write: if false;
    }

    match /circleCuts/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    // match /{allPaths=**} {
    //   allow read, write: if request.auth != null;
    // }
  }
}