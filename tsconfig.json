{
  "compilerOptions": {
    "target": "ES2023",
    "module": "CommonJS",
    "rootDir": "./src",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": false,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": false
  }
}